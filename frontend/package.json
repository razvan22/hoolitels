{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:selenium": "node node_modules/selenium-cucumber-js/index.js -f ./tests/selenium/features/ & exit 0",
    "test:feature": "node test-runner.js && exit 0",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@popperjs/core": "^2.3.3",
    "@smartweb/vue-flash-message": "^0.6.4",
    "core-js": "^3.6.4",
    "cucumber": "^6.0.5",
    "materialize-css": "^1.0.0-rc.2",
    "selenium": "^2.20.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "vue": "^2.6.11",
    "vue-flash-message": "^0.7.2",
    "vue-hotel-datepicker": "^2.7.8",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.6",
    "vue-social-sharing": "^2.4.7",
    "vue-star-rating": "^1.6.1",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "^4.3.1",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "~4.3.0",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "selenium-cucumber-js": "^1.7.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
